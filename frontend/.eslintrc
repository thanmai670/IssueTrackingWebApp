{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:prettier/recommended",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "printWidth": 120,
                "singleQuote": true,
                "endOfLine": "auto",
                "tabWidth": 4
            }
        ]
    }
}